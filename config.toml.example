metadata_dir = "/var/lib/garage/meta"
data_dir = "/var/lib/garage/data"
db_engine = "sqlite"
replication_factor = 2

rpc_bind_addr = "[::]:3901"
# IMPORTANT: This must match the Tailscale IP. 
# Since we are using Docker, run 'docker compose up' once, 
# check the Tailscale console for the IP, then update this file and restart.
rpc_public_addr = "100.x.y.z:3901" 
rpc_secret = "REPLACE_WITH_RPC_SECRET_FROM_ENV_FILE"

[s3_api]
s3_region = "garage"
api_bind_addr = "[::]:3900"
root_domain = ".s3.local"

# ENABLE ADMIN API for the Dashboard
[admin]
api_bind_addr = "[::]:3903"
admin_token = "REPLACE_WITH_ADMIN_TOKEN_FROM_ENV_FILE"
